{
  "name": "hyperlit-typescript",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "rm -rf dist .cache && env NODE_ENV=production parcel build --experimental-scope-hoisting --detailed-report --public-url ./ ./src/index.html",
    "check": "tsc"
  },
  "dependencies": {
    "hyperapp": "2.0.8",
    "hyperlit": "0.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "babel-plugin-hyperlit": "0.1.2",
    "json5": "2.1.3",
    "parcel-bundler": "1.12.4",
    "typescript": "3.9.6"
  }
}
